            <string>//Keyboard Navigation
if(keyboard_check(vk_up)){
    if(pos==1){pos=3;}
    if(pos==2){pos=1;}
    if(pos==3){pos=2;}
}
if(keyboard_check(vk_down)){
    if(pos==1){pos=2;}
    if(pos==2){pos=3;}
    if(pos==3){pos=1;}
}
//Keyboard Navigation
if(keyboard_check(vk_space)){
    if(pos==1){st=1;}
    if(pos==2){st=2;}
    if(pos==3){st=3;}
}

//Mouse Navigation1
