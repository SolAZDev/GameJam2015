if(mouse_y &gt;=p1y-64 &amp;&amp; mouse_y &lt;= p2y &amp;&amp; mouse_check_button(mb_any)){st=1;}
if(mouse_y &gt;=p2y &amp;&amp; mouse_y &lt;= p3y &amp;&amp; mouse_check_button(mb_any)){st=2;}
if(mouse_y &gt;=p3y &amp;&amp; mouse_y &lt;=p3y+64 &amp;&amp; mouse_check_button(mb_any)){st=3;}



//Debuging :: TitleScreen
if(st==1){draw_text(5,5,string(p1t));}
if(st==2){draw_text(5,5,string(p2t));}
if(st==3){draw_text(5,5,string(pt3));}

